{
  "name": "postboxd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:seed": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/seed.ts",
    "db:seed-screenings": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/seed-screenings.ts",
    "db:enrich": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/enrich-films.ts",
    "db:enrich-posters": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/enrich-posters.ts",
    "db:recheck-posters": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/enrich-posters.ts recheck",
    "db:cleanup-films": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/cleanup-films.ts",
    "db:classify-events": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/classify-events.ts",
    "db:cleanup-screenings": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/db/cleanup-screenings.ts",
    "scrape:bfi": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-bfi.ts",
    "scrape:pcc": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-pcc.ts",
    "scrape:ica": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-ica.ts",
    "scrape:barbican": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-barbican.ts",
    "scrape:rio": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-rio.ts",
    "scrape:curzon": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-curzon.ts",
    "scrape:picturehouse": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-picturehouse.ts",
    "scrape:everyman": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-everyman.ts",
    "scrape:genesis": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-genesis.ts",
    "scrape:peckhamplex": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-peckhamplex.ts",
    "scrape:nickel": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-nickel.ts",
    "scrape:electric": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-electric.ts",
    "scrape:lexi": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-lexi.ts",
    "scrape:garden": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-garden.ts",
    "scrape:close-up": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/run-close-up.ts",
    "scrape:independents": "npm run scrape:genesis && npm run scrape:peckhamplex && npm run scrape:nickel && npm run scrape:electric && npm run scrape:lexi && npm run scrape:garden && npm run scrape:close-up",
    "scrape:chains": "npm run scrape:curzon && npm run scrape:picturehouse && npm run scrape:everyman",
    "scrape:all": "npm run scrape:bfi && npm run scrape:pcc && npm run scrape:ica && npm run scrape:barbican && npm run scrape:rio && npm run scrape:chains && npm run scrape:independents",
    "load:bfi-manual": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/scrapers/load-bfi-manual.ts",
    "agents": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/agents/run-agents.ts all",
    "agents:links": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/agents/run-agents.ts links",
    "agents:health": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/agents/run-agents.ts health",
    "agents:enrich": "dotenv -e .env.local -- npx tsx -r tsconfig-paths/register src/agents/run-agents.ts enrich"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@base-ui/react": "^1.0.0",
    "@clerk/nextjs": "^6.36.5",
    "@posthog/ai": "^7.3.0",
    "@tanstack/react-query": "^5.90.12",
    "@turf/boolean-point-in-polygon": "^7.3.1",
    "@turf/turf": "^7.3.1",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "@vis.gl/react-google-maps": "^1.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "postgres": "^3.4.7",
    "posthog-js": "^1.310.1",
    "posthog-node": "^5.18.0",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/postcss": "^4",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-plugin-react-compiler": "1.0.0",
    "cheerio": "^1.1.2",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "openai": "^6.15.0",
    "playwright": "^1.57.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tailwindcss": "^4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
